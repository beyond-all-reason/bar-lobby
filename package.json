{
  "name": "bar-lobby",
  "version": "0.14.0",
  "private": true,
  "description": "Lobby client for the RTS game Beyond All Reason",
  "author": "Jazcash <jaspercashmore@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beyond-all-reason/bar-lobby.git"
  },
  "main": "./out/main/index.js",
  "scripts": {
    "dev": "electron-vite",
    "build": "electron-vite build",
    "compile": "electron-builder --config .electron-builder.config.js",
    "local-build": "npm run build && npm run compile",
    "fix": "npm run type-check && npm run tidy",
    "check": "npm run type-check && npm run tidy-check",
    "type-check": "vue-tsc --noEmit -p tsconfig.node.json && vue-tsc --noEmit -p tsconfig.web.json",
    "tidy": "npm run lint && npm run format",
    "tidy-check": "npm run lint-check && npm run format-check",
    "lint": "eslint . --ext .ts,.vue --fix",
    "lint-check": "eslint . --ext .ts,.vue",
    "format": "prettier --write src/**/*.{ts,vue,scss,json}",
    "format-check": "prettier --check src/**/*.{ts,vue,scss,json}",
    "test": "vitest run",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "preversion": "npm run check",
    "postversion": "git push && git push --tags"
  },
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344"
    }
  },
  "engines": {
    "node": "14.21.2"
  },
  "build": {
    "electronDist": "node_modules/electron/dist"
  },
  "dependencies": {
    "@iconify-icons/mdi": "^1.2.14",
    "@iconify/json": "^2.2.14",
    "@iconify/vue": "^4.1.0",
    "@vueuse/core": "^9.2.0",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "axios": "^0.24.0",
    "better-sqlite3": "^8.0.1",
    "date-fns": "^2.29.1",
    "env-paths": "^3.0.0",
    "flag-icons": "^6.1.1",
    "glob-promise": "^6.0.2",
    "howler": "^2.2.3",
    "jaz-ts-utils": "^6.2.0",
    "kysely": "^0.21.6",
    "luaparse": "^0.3.1",
    "node-7z": "^3.0.0",
    "node-self": "^1.0.1",
    "octokit": "^2.0.14",
    "pixi.js": "^7.1.4",
    "primeicons": "^6.0.1",
    "primevue": "^3.23.0",
    "sdfz-demo-parser": "^5.7.0",
    "spring-map-parser": "^6.0.3",
    "sqlite": "^4.1.1",
    "sqlite3": "^5.0.8",
    "steamworks.js": "0.0.27",
    "tachyon-client": "^6.0.5",
    "vue-i18n": "^9.2.2",
    "vue3-markdown-it": "^1.0.9",
    "vue3-popper": "^1.4.1"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron-toolkit/utils": "^1.0.2",
    "@sinclair/typebox": "^0.25.21",
    "@types/better-sqlite3": "^7.6.0",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/howler": "^2.2.4",
    "@types/luaparse": "^0.2.8",
    "@types/node-7z": "^2.1.4",
    "@types/uuid": "^9.0.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/compiler-sfc": "^3.2.29",
    "@vue/devtools": "^6.5.0",
    "bar-db": "^6.3.0",
    "cross-env": "^7.0.3",
    "date-fns": "^2.29.1",
    "electron": "^22.1.0",
    "electron-builder": "^23.6.0",
    "electron-devtools-installer": "^3.2.0",
    "electron-unhandled": "^4.0.1",
    "electron-updater": "^5.3.0",
    "electron-vite": "^1.0.11",
    "playwright": "^1.30.0",
    "prettier": "^2.6.2",
    "rimraf": "^4.1.2",
    "sass": "^1.52.1",
    "tsconfig-paths": "^4.1.2",
    "type-fest": "^3.5.6",
    "typescript": "^4.6.4",
    "unplugin-vue-router": "^0.5.4",
    "uuid": "^9.0.0",
    "vite": "^4.0.4",
    "vite-electron-plugin": "^0.7.4",
    "vite-plugin-electron": "^0.11.1",
    "vite-plugin-electron-renderer": "^0.11.4",
    "vite-plugin-esmodule": "^1.4.4",
    "vitest": "^0.28.4",
    "vue": "^3.2.37",
    "vue-router": "^4.1.6",
    "vue-tsc": "^1.0.9",
    "worker-plugin": "^5.0.1"
  }
}
